{
	"name": "vite-plugin-doctest",
	"version": "1.1.1",
	"description": "Vite plugin for documentation testing powered with vitest",
	"type": "module",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": ["dist"],
	"keywords": [
		"vite",
		"plugin",
		"doctest",
		"documentation",
		"test",
		"testing",
		"vitest"
	],
	"scripts": {
		"build": "tsup src/index.ts --format esm --dts",
		"check": "tsc --noEmit",
		"test": "vitest run"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ssssota/doc-vitest.git"
	},
	"author": "TOMIKAWA Sotaro <tommysouta@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ssssota/doc-vitest/issues"
	},
	"homepage": "https://github.com/ssssota/doc-vitest#readme",
	"peerDependencies": {
		"vitest": ">=2.0.0",
		"vite": ">=5.0.0",
		"typescript": ">=4.0.0"
	},
	"devDependencies": {
		"remark-parse": "^11.0.0",
		"tsup": "^8.3.5",
		"unified": "^11.0.5",
		"vite": "^6.0.0",
		"vitest": "catalog:",
		"magic-string": "^0.30.12",
		"typescript": "^5.6.3"
	}
}
