{
	"name": "vite-plugin-doctest",
	"version": "0.1.1",
	"description": "Vite plugin for documentation testing powered with vitest",
	"type": "commonjs",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": ["dist"],
	"keywords": [
		"vite",
		"plugin",
		"doctest",
		"documentation",
		"test",
		"testing",
		"vitest"
	],
	"scripts": {
		"build": "tsup src/index.ts --format cjs --dts",
		"check": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ssssota/doc-vitest.git"
	},
	"author": "TOMIKAWA Sotaro <tommysouta@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ssssota/doc-vitest/issues"
	},
	"homepage": "https://github.com/ssssota/doc-vitest#readme",
	"peerDependencies": {
		"vitest": ">=0.29.0"
	},
	"devDependencies": {
		"remark-parse": "^10.0.2",
		"tsup": "^8.0.0",
		"unified": "^10.1.2",
		"vite": "^5.0.0",
		"vitest": "^0.34.0"
	},
	"dependencies": {
		"magic-string": "^0.30.3",
		"typescript": "^4.0.0 || ^5.0.0",
		"vite": "^4.0.0 || ^5.0.0-0"
	}
}
